{
    "version": 2,
    "builds": [
      {
        "src": "server/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      }
    ]
  }
  